

#include <stdio.h>
#include <stdlib.h>
struct
{
 int id;
 char name[51];
 char lastName[51];
 float salary;
 int idlocalidad;
 int isEmpty;
}typedef Employee;

struct
{
 char localidad[51];
 int idlocalidad;

}typedef eLocalidad;


int main(void) {
	int id=0;
int opcion;
int i;

Employee personas[4];
eLocalidad localidad[4];
	do{

		id++;
		for (i = 0; i < 3; ++i) {
			puts("ingrese su nombre");
			fflush(stdin);
			gets(personas[i].name);

			puts("ingrese la localidad");
			fflush(stdin);
			gets(localidad[i].idlocalidad);
		}




	}while(opcion!=10);

}
