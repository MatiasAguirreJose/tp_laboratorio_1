int IngreseNumero(int numero){
	int descuento;
	int total;


	printf("ingrese el importe");
	scanf("%d",&numero);

	printf("cuando de descuento desea aplicar");
	scanf("%d",&descuento);

	total = (numero*descuento)/100;


	return total;

}

void Descuento(int numeros){


	printf("el numero con descuento es %d",numeros);


	}


