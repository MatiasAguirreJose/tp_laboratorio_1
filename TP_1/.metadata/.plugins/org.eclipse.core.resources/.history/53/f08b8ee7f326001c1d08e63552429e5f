void Descuento(void);
int main(void) {

	setbuf(stdout, NULL);
	int numero;
	IngreseNumero(numero);
	void Descuento(void)
	return EXIT_SUCCESS;
}

int IngreseNumero(int numero){
	int descuento;
	int total;


	printf("ingrese el importe");
	scanf("%d",&numero);

	printf("cuando de descuento desea aplicar");
	scanf("%d",&descuento);

	total = (numero*descuento)/100;


	return total;

}

void Descuento(void){
	int numero;
	int final;
	final=IngreseNumero(numero);

	printf("el numero con descuento es %d",final);


	}


